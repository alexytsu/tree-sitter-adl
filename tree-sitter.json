{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "adl",
      "camelcase": "Adl",
      "title": "ADL",
      "scope": "source.adl",
      "file-types": [
        "adl"
      ],
      "injection-regex": "^adl$",
      "class-name": "TreeSitterAdl"
    }
  ],
  "metadata": {
    "version": "0.2.0",
    "license": "MIT",
    "description": "A tree-sitter grammar for Algebraic Data Language (ADL)",
    "authors": [
      {
        "name": "Alex Su",
        "email": "alexanderytsu@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/alexytsu/tree-sitter-adl"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
