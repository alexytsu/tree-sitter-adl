{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "adl",
  "rules": {
    "source_file": {
      "type": "SYMBOL",
      "name": "module_definition"
    },
    "module_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "module"
        },
        {
          "type": "SYMBOL",
          "name": "module_name"
        },
        {
          "type": "SYMBOL",
          "name": "module_body"
        }
      ]
    },
    "module_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "module_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "struct_definition"
              },
              {
                "type": "SYMBOL",
                "name": "union_definition"
              },
              {
                "type": "SYMBOL",
                "name": "type_definition"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "};"
        }
      ]
    },
    "type_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "SYMBOL",
          "name": "type_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "type_body"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "type_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "type_body": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "struct_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "struct"
        },
        {
          "type": "SYMBOL",
          "name": "struct_name"
        },
        {
          "type": "SYMBOL",
          "name": "struct_body"
        }
      ]
    },
    "struct_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "struct_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "struct_field"
          }
        },
        {
          "type": "STRING",
          "value": "};"
        }
      ]
    },
    "struct_field": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field_type"
        },
        {
          "type": "SYMBOL",
          "name": "field_name"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "union_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "union"
        },
        {
          "type": "SYMBOL",
          "name": "union_name"
        },
        {
          "type": "SYMBOL",
          "name": "union_body"
        }
      ]
    },
    "union_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "union_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "union_field"
          }
        },
        {
          "type": "STRING",
          "value": "};"
        }
      ]
    },
    "union_field": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field_type"
        },
        {
          "type": "SYMBOL",
          "name": "field_name"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "field_type": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "field_name": {
      "type": "SYMBOL",
      "name": "identifier"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z][a-zA-Z0-9_]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}